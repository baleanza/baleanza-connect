{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/check", "dest": "/check.js" },
    { "src": "/", "dest": "/monomarket-offers.js" },
    { "src": "/api/monomarket-offers", "dest": "/monomarket-offers.js" },
    { "src": "/api/monomarket-stock", "dest": "/monomarket-stock.js" },
    { "src": "/api/murkit-webhook", "dest": "/murkit-webhook.js" },
    { "src": "/api/debug-wix", "dest": "/debug-wix.js" }
  ]
}
